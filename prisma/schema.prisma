generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Customer {
  id     Int    @id @default(autoincrement())
  name   String
  email  String @unique
  orders Order[]
}

model Order {
  id          Int      @id @default(autoincrement())
  customer    Customer @relation(fields: [customerId], references: [id])
  customerId  Int
  product     String
  amount      Float
}
